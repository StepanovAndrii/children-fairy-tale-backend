{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost",
  "Cors": {
    "Policies": [
      {
        "Name": "Default",
        "Origins": [
          "http://localhost:4200",
          "https://localhost:4200"
        ],
        "Headers": [ "*" ],
        "Methods": [ "*" ],
        "AllowCredentials": true
      }
    ]
  },
  "Authentication": {
    "RedirectUrl": "https://localhost:4200"
  }
}